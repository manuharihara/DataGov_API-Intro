@model DataGov_API_Intro.Models.Parks

@{
    ViewData["Title"] = "Index";
}

<h2>Parks Data :</h2>

<div><b>Table Format Data Displaying in ForEach</b><br /></div>
<table class="table table-bordered table-responsive table-hover">
    <tr>

        <th>parkCode </th>
        <th>fullName </th>
        <th>latLong </th>

    </tr>
    @foreach (DataGov_API_Intro.Models.Park park in Model.data)
    { 
    <tr>

        <td>@park.parkCode</td>
        <td>@park.fullName</td>
        <td>@park.latLong</td>


    </tr>

    }
</table>  
